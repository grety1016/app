{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": false, 
    "noEmit": true, 
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@/assets":["src/assets"]
    },
  },  
  "include": ["vite.config.ts","src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "postcss.config.cjs","src/**/*.css"]

}
